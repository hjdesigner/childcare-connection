service cloud.firestore {
  match /databases/{database}/documents {
    match /Users/{user}  {
      allow read: if true;
      allow write: if request.auth.uid != null;
    }
    match /Cases/{caseData}  {
      allow read, write: if request.auth.uid != null;
    }
    match /CaseIds/{id}  {
      allow read, write: if request.auth.uid != null;
    }
    match /Providers/{prov} {
      allow read, write: if request.auth.uid != null;
    }
  }
}
